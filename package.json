    "@sentry/nextjs": "^10.20.0",
    "@stripe/stripe-js": "^8.0.0",
    "ai": "^5.0.76",
    "next": "14.2.5",
    "next-auth": "^4.24.7",
    "next-axiom": "^1.9.3",
    "openai": "^4.52.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "stripe": "^19.1.0",
