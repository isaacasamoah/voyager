    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma migrate deploy",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "rm -rf .next"
  },
  "dependencies": {
